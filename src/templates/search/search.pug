extends ../include/defaults

block config
    - rootBase = '../'
    - title = 'Search'
    - breadcrumb = 'Search'
    - pageClass = 'search'
    - imgPath = rootBase + 'assets/images/'
    - cssPath = rootBase + 'assets/css/internal/'
    - jsPath  = rootBase + 'assets/js/'

block stylesheet
    link(rel='stylesheet', href = rootBase + 'assets/css/internals/search.min.css')
        
block javascript

block content
    include ../include/components/_breadcrumb
    include ../include/partials/search/_search
